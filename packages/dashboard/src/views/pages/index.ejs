<div class="flex flex-col gap-8">
  <div class="form-control w-48">
    <label class="label label-text">Jobs in the last</label>
    <select id="graph-range" name="range" class="select select-bordered">
      <option value="12m" selected>12 minutes</option>
      <option value="12h">12 hours</option>
      <option value="12d">12 days</option>
    </select>
  </div>

  <!-- Stats -->
  <%- include('../partials/dashboard-stats') %>

  <!-- Graph -->
  <div class="bg-gray-800 p-6 rounded shadow border border-gray-700 flex flex-col gap-2">
    <div class="w-full h-96">
      <canvas id="jobsTimeline" class="w-full h-full"></canvas>
    </div>
  </div>
</div>

<script src="<%= basePath %>/public/js/dashboard.js"></script>
